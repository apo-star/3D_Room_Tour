{
  "source": {
    "include": ["src/3d/pano"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs|three-libs)"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "default": {
      "outputSourceFiles": false
    }
  },
  "opts": {
    "recurse": true,
    "destination": "./build/documentation/"
  }
}
